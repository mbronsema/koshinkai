<!DOCTYPE html>
<html>
<head>
  <title>Koshinkai</title>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag "http://maps.google.com/maps/api/js?sensor=true&.js" %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <script type="text/javascript"> 
    var $buoop = {}; 
    $buoop.ol = window.onload; 
    window.onload=function(){ 
     try {if ($buoop.ol) $buoop.ol();}catch (e) {} 
     var e = document.createElement("script"); 
     e.setAttribute("type", "text/javascript"); 
     e.setAttribute("src", "//browser-update.org/update.js"); 
     document.body.appendChild(e); 
    } 
  </script> 
  
</head>
<body>
<section id="header">
  <header >
  <%= content_tag(:div, alert, class: "alert alert-danger container custom-alert animated pulse") if(flash[:alert]) %>
  <%= content_tag(:div, notice, class: "alert alert-success container custom-alert animated pulse") if(flash[:notice]) %>
  <% if current_page? root_path %>
  <iframe width="560" height="315" src="//www.youtube.com/embed/f5Qd1weoc_I?controls=0&modestbranding=1&rel=0&showinfo=0&autohide=0" frameborder="0" allowfullscreen></iframe>
<!-- <iframe id="ytplayer" type="text/html" width="560" height="315" src="https://www.youtube.com/embed/f5Qd1weoc_I?controls=0&modestbranding=1&rel=0&showinfo=0&autohide=0" frameborder="0" allowfullscreen></iframe>
 -->  <% end %>
    <%= render "layouts/profile" %>
  </header>
  <!-- Nieuwe menu -->
  <%= render "layouts/menu" %>
</section>
<article class="container">

<section>

<%= yield %>  
<section class="row" id="special">
  <div class="col-md-4">
      <div id="maps" ><img /></div>
  </div>
  <div class="col-md-4"><p>Aikido Koshinkai<br/>Emmakade 59 <br/> Leeuwarden</p></div>
  <div class="col-md-4"></div>

</section>

</section>
</article>

<footer><%= render "layouts/footer" %></footer>
</body>
</html>
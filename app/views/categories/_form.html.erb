<%= form_for @category do |c| %>
<%= c.label :menuname, "Menu naam" %><br /> <%= c.select(:menuname, options_for_select(['Home', 'Prijzen', 'Aikido', 'Koshinkai', 'Agenda', 'Media', 'Contact'])) %><br />
<%= c.label :submenuname, "Submenu naam" %><br /> <%= c.text_field :submenuname %><br />
<%= c.submit %>
<% end %>